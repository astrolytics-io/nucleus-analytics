  },
  "keywords": [
    "analytics",
    "tracking",
    "crash",
    "bugs",
    "nw.js",
    "electron"
  ],
  "author": "Nucleus.sh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nucleus-sh/nucleus-analytics/issues"
  },
  "homepage": "https://nucleus.sh",
  "dependencies": {
